
REM AMIHAL ACTOR_ACTRESS

  CREATE TABLE "AMIHAL"."ACTOR_ACTRESS" 
   (	"NAME" VARCHAR2(50 BYTE), 
	"SAG_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "ACTOR_ACTRESS_PK" PRIMARY KEY ("SAG_NUMBER") ENABLE
   ) ;
 
 
REM AMIHAL DIRECTED_BY

  CREATE TABLE "AMIHAL"."DIRECTED_BY" 
   (	"PRODUCTION_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"SAG_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "DIRECTED_BY_PK" PRIMARY KEY ("PRODUCTION_NUMBER", "SAG_NUMBER") ENABLE, 
	 CONSTRAINT "FEAT_DIRECTOR_SAG_NUMBER" FOREIGN KEY ("SAG_NUMBER")
	  REFERENCES "AMIHAL"."DIRECTOR" ("SAG_NUMBER") ENABLE, 
	 CONSTRAINT "FEAT_FILM_PRODUCTION_NUMBER" FOREIGN KEY ("PRODUCTION_NUMBER")
	  REFERENCES "AMIHAL"."FILM" ("PRODUCTION_NUMBER") ENABLE
   ) ;
 
 
REM AMIHAL DIRECTOR

  CREATE TABLE "AMIHAL"."DIRECTOR" 
   (	"NAME" VARCHAR2(50 BYTE), 
	"SAG_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "DIRECTOR_PK" PRIMARY KEY ("SAG_NUMBER") ENABLE
   ) ;
 
 
REM AMIHAL FEATURING

  CREATE TABLE "AMIHAL"."FEATURING" 
   (	"PRODUCTION_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"SAG_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "FEATURING_PK" PRIMARY KEY ("PRODUCTION_NUMBER", "SAG_NUMBER") ENABLE, 
	 CONSTRAINT "FEATURING_ACTORACTRESS_SAG_NUM" FOREIGN KEY ("SAG_NUMBER")
	  REFERENCES "AMIHAL"."ACTOR_ACTRESS" ("SAG_NUMBER") ENABLE, 
	 CONSTRAINT "FEATURING_FILM_PROD_NUM" FOREIGN KEY ("PRODUCTION_NUMBER")
	  REFERENCES "AMIHAL"."FILM" ("PRODUCTION_NUMBER") ENABLE
   ) ;
 
 
REM AMIHAL FILM

  CREATE TABLE "AMIHAL"."FILM" 
   (	"MONTH" VARCHAR2(50 BYTE), 
	"DAY" VARCHAR2(50 BYTE), 
	"YEAR" VARCHAR2(50 BYTE), 
	"RATING" VARCHAR2(50 BYTE), 
	"METACRITIC_SCORE" VARCHAR2(50 BYTE), 
	"PRODUCTION_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"DISTRO_COMPANY_NAME" VARCHAR2(50 BYTE), 
	"SAG_NUMBER" VARCHAR2(50 BYTE), 
	"TITLE" VARCHAR2(50 BYTE), 
	"GENRE" VARCHAR2(50 BYTE), 
	 CONSTRAINT "FILM_PK" PRIMARY KEY ("PRODUCTION_NUMBER") ENABLE, 
	 CONSTRAINT "FILM_DISTRO_COMPANY_NAME" FOREIGN KEY ("DISTRO_COMPANY_NAME")
	  REFERENCES "AMIHAL"."US_DISTROBUTION_COMPANY" ("NAME") ENABLE, 
	 CONSTRAINT "FILM_ACTOR_ACTRESS_SAG" FOREIGN KEY ("SAG_NUMBER")
	  REFERENCES "AMIHAL"."ACTOR_ACTRESS" ("SAG_NUMBER") ENABLE
   ) ;
 
 
REM AMIHAL MADE_BY

  CREATE TABLE "AMIHAL"."MADE_BY" 
   (	"PRODUCTION_NUMBER" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	"PRODUCTION_NAME" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "MADE_BY_PK" PRIMARY KEY ("PRODUCTION_NUMBER", "PRODUCTION_NAME") ENABLE, 
	 CONSTRAINT "MADE_BY_PROD_NUMBER" FOREIGN KEY ("PRODUCTION_NUMBER")
	  REFERENCES "AMIHAL"."FILM" ("PRODUCTION_NUMBER") ENABLE, 
	 CONSTRAINT "MADE_BY_PROD_NAME" FOREIGN KEY ("PRODUCTION_NAME")
	  REFERENCES "AMIHAL"."PRODUCTION_COMPANY" ("PRODUCTION_NAME") ENABLE
   ) ;
 
 
REM AMIHAL PRODUCTION_COMPANY

  CREATE TABLE "AMIHAL"."PRODUCTION_COMPANY" 
   (	"PRODUCTION_NAME" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "PRODUCTION_COMPANY_PK" PRIMARY KEY ("PRODUCTION_NAME") ENABLE
   ) ;
 
 
REM AMIHAL US_DISTROBUTION_COMPANY

  CREATE TABLE "AMIHAL"."US_DISTROBUTION_COMPANY" 
   (	"NAME" VARCHAR2(50 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "US_DISTROBUTION_COMPANY_PK" PRIMARY KEY ("NAME") ENABLE
   ) ;
 